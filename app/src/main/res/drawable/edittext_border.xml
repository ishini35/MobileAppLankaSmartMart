<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- Border/Stroke -->
    <stroke
        android:width="1dp"
        android:color="#4CAF50" />

    <!-- Rounded Corners -->
    <corners
        android:radius="8dp" />

    <!-- Background Color -->
    <solid
        android:color="#FFFFFF" />

    <!-- Padding Inside -->
    <padding
        android:left="16dp"
        android:top="14dp"
        android:right="16dp"
        android:bottom="14dp" />
</shape>